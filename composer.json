{
    "name": "civicamente/logros-cm",
    "description": "Módulo exclusivo para civicamente.cl",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jorge Vásquez",
            "email": "nashowii@gmail.com"
        }
    ],
    "minimum-stability": "beta",
    "autoload": {
        "classmap": [
            "src/Migrations"
        ],

        "psr-4": {
            "Civicamente\\LogrosCm\\": "src/",
            "Civicamente\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Civicamente\\LogrosCm\\AchievementsServiceProvider"
            ]
        }
    },
    "require": {
        "ramsey/uuid": "^3 || ^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8",
        "laravel/laravel": "^6.0|^7.0|^8.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/nashowii/logros-cm"
        }
    ]
}
